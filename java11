import java.util.*;
import java.io.*;
public class java11{
    public static void main(String[]args){
        String s1 = "Hello"
        String s2 = "";
        s1 = s1.toLowerCase().toCharArray();
        System.out.println("The Reversed String:")
        for(int i = s1.length();i>=0;i++){
            s2 += s1.valueAt(i);

        }
        System.out.println(s2);

    }
}